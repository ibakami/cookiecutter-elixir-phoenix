{
	"repository": {},
	"license": "MIT",
	"scripts": {
		"deploy": "webpack --mode production",
		"watch": "webpack --mode development --watch",
		"format:prettier":
			"prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
		"lint:scripts": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src/",
		"typecheck": "tsc --noEmit"
	},
	"importSort": {
		".js, .jsx": {
			"parser": "babylon",
			"style": "module"
		},
		".ts, .tsx": {
			"parser": "typescript",
			"style": "module"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsc --noEmit && lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"**/*.{css,gql,graphql,html,json,less,md,mdx,scss,vue,yaml,yml}": ["prettier --write", "git add"],
			"**/*.{js,jsx,ts,tsx}": [
				"import-sort --write",
				"prettier --write",
				"eslint --cache --ext .js,.jsx,.ts,.tsx --fix",
				"git add"
			]
		},
		"concurrent": false
	},
	"dependencies": {
		"@types/styled-components": "^4.1.18",
		"apollo-boost": "^0.4.3",
		"axios": "^0.19.0",
		"axios-hooks": "^1.1.5",
		"graphql": "^14.4.2",
		"graphql-tag": "^2.10.1",
		"lodash": "^4.17.14",
		"phoenix": "file:../../../deps/phoenix",
		"phoenix_html": "file:../../../deps/phoenix_html",
		"react": "^16.8.6",
		"react-apollo": "^2.5.8",
		"react-dom": "^16.8.6",
		"semantic-ui-react": "^0.87.3",
		"styled-components": "^4.3.2"
	},
	"babel": {
		"presets": ["@babel/env", "@babel/preset-typescript", "@babel/preset-react"],
		"plugins": ["lodash", "@babel/plugin-transform-runtime", "@babel/syntax-dynamic-import"]
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.5.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.4",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.5.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@reach/router": "^1.2.1",
		"@types/graphql": "^14.2.3",
		"@types/lodash": "^4.14.136",
		"@types/reach__router": "^1.2.4",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.8.4",
		"@typescript-eslint/eslint-plugin": "^1.12.0",
		"@typescript-eslint/parser": "^1.12.0",
		"babel-loader": "^8.0.0",
		"babel-plugin-lodash": "^3.3.4",
		"clean-webpack-plugin": "^3.0.0",
		"compression-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "^2.1.1",
		"eslint": "^6.0.1",
		"eslint-config-airbnb-typescript": "^4.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-formatter-pretty": "^2.1.1",
		"eslint-import-resolver-webpack": "^0.11.1",
		"eslint-plugin-eslint-comments": "^3.1.2",
		"eslint-plugin-import": "^2.18.0",
		"eslint-plugin-jest": "^22.10.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.14.2",
		"eslint-plugin-react-hooks": "^1.6.1",
		"eslint-plugin-unicorn": "^9.1.1",
		"file-loader": "^4.0.0",
		"husky": "^3.0.0",
		"image-loader": "^0.0.1",
		"import-sort": "^6.0.0",
		"import-sort-cli": "^6.0.0",
		"import-sort-parser-babylon": "^6.0.0",
		"import-sort-parser-typescript": "^6.0.0",
		"import-sort-style-module": "^6.0.0",
		"lint-staged": "8.1.7",
		"lodash-webpack-plugin": "^0.11.5",
		"mini-css-extract-plugin": "^0.4.0",
		"node-sass": "^4.12.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"sass-loader": "^7.1.0",
		"stylelint": "^10.1.0",
		"stylelint-config-recommended": "^2.2.0",
		"terser-webpack-plugin": "^1.3.0",
		"typescript": "^3.5.3",
		"uglifyjs-webpack-plugin": "^1.2.4",
		"url-loader": "^2.0.1",
		"webpack": "4.4.0",
		"webpack-assets-manifest": "^3.1.1",
		"webpack-cli": "^2.0.10",
		"workbox-webpack-plugin": "^4.3.1"
	}
}
